{
  "expo": {
    "name": "Vybe",
    "slug": "vybe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Splash_v2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-9712054637149885~6820288476"
      },
      "bundleIdentifier": "com.canonal.vybe"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "833b892b-caed-4283-ade1-ce45b619f990"
      }
    },
    "owner": "canonal"
  },
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-9712054637149885~6820288476"
  }
}
